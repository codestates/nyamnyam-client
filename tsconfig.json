{
  "compilerOptions": {
    "module": "esnext",
    "target":"ESNext",
    "jsx": "react",
    "esModuleInterop": true,
    "strict": true,
    "outDir": "dist",
    "lib": ["es6", "es2017"],
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
  }
}
